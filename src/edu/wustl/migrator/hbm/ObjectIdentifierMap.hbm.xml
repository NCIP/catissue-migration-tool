<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="edu.wustl.migrator.metadata.ObjectIdentifierMap"
        table="MIGRATION_MAPPING"
        lazy="false"
    >
    <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">MIGRATION_MAPPING_SEQ</param>
            </generator>
        </id>
        <property
            name="className"
            type="string"
            update="true"
            insert="true"
            column="object_classname"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="oldId"
            type="long"
            update="true"
            insert="true"
            column="old_Id"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="newId"
            type="long"
            update="true"
            insert="true"
            column="new_Id"
            length="30"
            not-null="false"
            unique="false"
        />

       

    </class>

</hibernate-mapping>

